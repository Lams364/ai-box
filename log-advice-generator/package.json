{
  "name": "log-advice-generator",
  "displayName": "Log Advice Generator",
  "description": "Generates log advice based on selected code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onCommand:log-advice-generator.generateLogAdvice"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "log-advice-generator.generateLogAdvice",
        "title": "Generate Log Advice",
        "category": "log-advice-generator"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "log-advice-generator.generateLogAdvice",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "vscode": "^1.1.34"
  }
}
